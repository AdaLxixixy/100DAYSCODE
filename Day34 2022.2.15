import processing.sound.*;

SoundFile s;

FFT fft;

void setup(){
  size(600, 800);
  s = new SoundFile(this,"Spring_In_My_Step.mp3");
  s.loop();
 
  fft = new FFT(this, 512);
  fft.input(s);
}

void draw(){
  background(255);
  
  fft.analyze();
  
  for(int i = 0; i< fft.size(); i++){
    stroke(0);
    float y = map(fft.spectrum[i], 0 , 1, height * 0.75, 0);
    line(i, height * 0.75, i, y);
  }
}
