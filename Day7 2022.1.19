1.
float t=0;

void setup(){
  size (600, 600);
  frameRate(1);
}

void draw(){
   background(255);
   for(int i = 0; i<width; i+=6)
   {
     stroke(0);
     strokeWeight(3);
     line(i, 0, i, height*noise(t));
     t+=0.1;
   }
}








